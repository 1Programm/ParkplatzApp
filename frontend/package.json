{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --public-host=http://localhost:4200",
    "build": "ng build --aot --localize && npm run move-de-files",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-prod": "ng build --configuration production --localize && npm run move-de-files",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless --code-coverage=true",
    "smoketest": "npm run build-prod && npm run test-headless && npm run xi18n",
    "move-de-files": "node move-de-files.js",
    "xi18n": "ng extract-i18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.11",
    "@angular/common": "14.2.11",
    "@angular/compiler": "14.2.11",
    "@angular/core": "14.2.11",
    "@angular/forms": "14.2.11",
    "@angular/platform-browser": "14.2.11",
    "@angular/platform-browser-dynamic": "14.2.11",
    "@angular/router": "14.2.11",
    "rxjs": "7.5.5",
    "tslib": "2.3.1",
    "zone.js": "0.11.5",
    "@ihk-gfi/lux-components": "14.0.0",
    "@ihk-gfi/lux-components-theme": "14.0.0",
    "@ihk-gfi/lux-components-icons-and-fonts": "1.1.0",
    "@angular/localize": "14.2.11",
    "@angular/cdk": "14.2.7",
    "@angular/material": "14.2.7",
    "@angular/flex-layout": "14.0.0-beta.40",
    "@fortawesome/fontawesome-free": "5.15.4",
    "material-design-icons-iconfont": "6.5.0",
    "dompurify": "2.3.7",
    "marked": "4.0.15",
    "hammerjs": "2.0.8",
    "ng2-pdf-viewer": "8.0.1",
    "pdfjs-dist": "2.13.216",
    "ngx-cookie-service": "14.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.10",
    "@angular/cli": "14.2.10",
    "@angular/compiler-cli": "14.2.11",
    "@ihk-gfi/lux-components-update": "^14.0.0",
    "@types/jasmine": "4.0.3",
    "jasmine-core": "4.1.1",
    "karma": "6.3.19",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "typescript": "4.7.3",
    "@angular/language-service": "14.2.11",
    "@angular/elements": "14.2.11",
    "@types/node": "16.11.34",
    "fs-extra": "^10.0.0",
    "del": "^6.0.0"
  }
}
